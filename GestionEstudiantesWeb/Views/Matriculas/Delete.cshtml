@model GestionEstudiantesWeb.Models.Matricula

<h4>¿Estas seguro de eliminar la matricula?</h4>
<p></p>
<div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Fecha)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Fecha)
        </dd>
        <dt class = "col-sm-2">Estudiante </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.oEstudiante.NombreCompleto)
        </dd>
        <dt class = "col-sm-2">Materia</dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.oMateria.Nombre)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdMatricula" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-primary">Cancelar</a>
    </form>
</div>
